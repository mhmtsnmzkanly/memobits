// FizzBuzz: loop, if, break, mod

var i = 1;
loop {
    if (i > 15) { break; }
    if (((i % 3) == 0) && ((i % 5) == 0)) {
        native::print("FizzBuzz")
    } else {
        if ((i % 3) == 0) {
            native::print("Fizz")
        } else {
            if ((i % 5) == 0) {
                native::print("Buzz")
            } else {
                let n = i;
                native::print(`{n}`)
            }
        }
    };
    i = (i + 1)
}
