// Ok/Err literal
let ok = Ok(42);
let err = Err("hata");
native::print(ok);
native::print(err);

fn safe_div(a: Int, b: Int) -> Result<Int, String> {
    if (b == 0) { return Err("division by zero"); }
    return Ok(a / b);
}

let r1 = safe_div(10, 2);
let r2 = safe_div(10, 0);

native::print(r1);
native::print(r2);
